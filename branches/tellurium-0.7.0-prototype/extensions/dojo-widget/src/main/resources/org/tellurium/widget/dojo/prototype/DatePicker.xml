<TABLE cellspacing="0" cellpadding="0" class="calendarContainer">
		<THEAD>
			<TR>
				<TD valign="top" class="monthWrapper">
					<TABLE cellspacing="0" cellpadding="0" border="0" class="monthContainer">
						<TBODY><TR>
							<TD valign="top" class="monthCurve monthCurveTL"/>
							<TD valign="top" class="monthLabelContainer">
								<SPAN class="incrementControl increase" dojoattachevent="onClick: onIncrementWeek;" dojoattachpoint="increaseWeekNode">
									<IMG style="width: 7px; height: 5px;" alt="↓" src="../../src/widget/templates/images/incrementMonth.png"/>
								</SPAN>
								<SPAN class="incrementControl increase" dojoattachevent="onClick: onIncrementMonth;" dojoattachpoint="increaseMonthNode">
									<IMG dojoattachpoint="incrementMonthImageNode" alt="↓" src="../../src/widget/templates/images/incrementMonth.png"/>
								</SPAN>
								<SPAN class="incrementControl decrease" dojoattachevent="onClick: onIncrementWeek;" dojoattachpoint="decreaseWeekNode">
									<IMG style="width: 7px; height: 5px;" alt="↑" src="../../src/widget/templates/images/decrementMonth.png"/>
								</SPAN>
								<SPAN class="incrementControl decrease" dojoattachevent="onClick: onIncrementMonth;" dojoattachpoint="decreaseMonthNode">
									<IMG dojoattachpoint="decrementMonthImageNode" alt="↑" src="../../src/widget/templates/images/decrementMonth.png"/>
								</SPAN>
								<SPAN class="month" dojoattachpoint="monthLabelNode">April</SPAN>
							</TD>
							<TD valign="top" class="monthCurve monthCurveTR"/>
						</TR>
					</TBODY></TABLE>
				</TD>
			</TR>
		</THEAD>
		<TBODY>
			<TR>
				<TD colspan="3">
					<TABLE cellspacing="0" cellpadding="0" border="0" class="calendarBodyContainer">
						<THEAD>
							<TR>
								<TD>S</TD>
								<TD>M</TD>
								<TD>T</TD>
								<TD>W</TD>
								<TD>T</TD>
								<TD>F</TD>
								<TD>S</TD>
							</TR>
						</THEAD>
						<TBODY dojoattachevent="onClick: onSetDate;" dojoattachpoint="calendarDatesContainerNode">
							<TR dojoattachpoint="calendarRow0">
								<TD class="currentMonth">1</TD>
								<TD class="currentMonth">2</TD>
								<TD class="currentMonth">3</TD>
