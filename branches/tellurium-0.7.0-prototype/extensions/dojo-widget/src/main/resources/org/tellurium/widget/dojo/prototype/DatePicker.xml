								<TD class="currentMonth">8</TD>
								<TD class="currentMonth">9</TD>
								<TD class="currentMonth">10</TD>
								<TD class="currentMonth">11</TD>
								<TD class="currentMonth">12</TD>
								<TD class="selectedItem currentMonth">13</TD>
								<TD class="currentMonth">14</TD>
							</TR>
							<TR dojoattachpoint="calendarRow2">
								<TD class="currentMonth">15</TD>
								<TD class="currentMonth">16</TD>
								<TD class="currentMonth">17</TD>
								<TD class="currentMonth">18</TD>
								<TD class="currentMonth">19</TD>
								<TD class="currentMonth">20</TD>
								<TD class="currentMonth">21</TD>
							</TR>
							<TR dojoattachpoint="calendarRow3">
								<TD class="currentMonth">22</TD>
								<TD class="currentMonth">23</TD>
								<TD class="currentMonth">24</TD>
								<TD class="currentMonth">25</TD>
								<TD class="currentMonth">26</TD>
								<TD class="currentMonth">27</TD>
								<TD class="currentMonth">28</TD>
							</TR>
							<TR dojoattachpoint="calendarRow4">
								<TD class="currentMonth">29</TD>
								<TD class="currentMonth">30</TD>
								<TD class="nextMonth">1</TD>
								<TD class="nextMonth">2</TD>
								<TD class="nextMonth">3</TD>
								<TD class="nextMonth">4</TD>
								<TD class="nextMonth">5</TD>
							</TR>
							<TR dojoattachpoint="calendarRow5">
								<TD class="nextMonth">6</TD>
								<TD class="nextMonth">7</TD>
								<TD class="nextMonth">8</TD>
